{
	"name": "model-scraper",
	"private": true,
	"type": "module",
	"scripts": {
		"dev": "CLOUDFLARE_ENV=development vite",
		"start": "npm run dev",
		"build": "vite build",
		"preview": "npm run build && vite preview",
		"deploy": "npm run build && npm run deploy:production",
		"deploy:production": "wrangler deploy -e production",
		"deploy:staging": "wrangler deploy -e staging",
		"format": "biome format --write",
		"test": "vitest",
		"cf-typegen": "wrangler types",
		"lint": "biome lint --error-on-warnings",
		"lint:fix": "biome lint --fix",
		"test:ci": "vitest --watch=false",
		"type-check": "tsc --noEmit"
	},
	"dependencies": {
		"@ai-sdk/openai": "^3.0.1",
		"agents": "^0.3.1",
		"ai": "^6.0.3",
		"hono": "^4.11.4",
		"workers-ai-provider": "^3.0.2",
		"zod": "^4.2.1"
	},
	"devDependencies": {
		"@biomejs/biome": "^2.3.10",
		"@cloudflare/vite-plugin": "^1.21.1",
		"@cloudflare/vitest-pool-workers": "^0.12.5",
		"typescript": "5.9.3",
		"vite": "^7.3.0",
		"vitest": "~3.2.4",
		"wrangler": "^4.59.3"
	}
}
