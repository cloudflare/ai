{
  "name": "@harshil1712/tanstack-cf-ai-adapter",
  "version": "0.0.1",
  "description": "Cloudflare AI Gateway adapter for TanStack AI - route requests through Cloudflare's AI Gateway with caching, rate limiting, and observability",
  "author": "Harshil Agrawal",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/harshil1712/tanstack-cf-ai-adapter"
  },
  "keywords": [
    "tanstack",
    "ai",
    "cloudflare",
    "ai-gateway",
    "adapter",
    "workers"
  ],
  "type": "module",
  "main": "./dist/index.js",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js"
    }
  },
  "files": [
    "dist",
    "src",
    "README.md"
  ],
  "scripts": {
    "build": "tsc",
    "dev": "tsc --watch",
    "prepublishOnly": "npm run build",
    "changeset": "changeset",
    "changeset:version": "changeset version && npm install",
    "changeset:publish": "changeset publish"
  },
  "dependencies": {
    "@anthropic-ai/sdk": "^0.71.2",
    "@cloudflare/workers-types": "^4.20251205.0",
    "@tanstack/ai": "^0.0.2",
    "@tanstack/ai-anthropic": "^0.0.2",
    "@tanstack/ai-openai": "^0.0.2",
    "openai": "^6.10.0"
  },
  "peerDependencies": {
    "@tanstack/ai": "^0.0.2"
  },
  "devDependencies": {
    "@changesets/changelog-github": "^0.5.2",
    "@changesets/cli": "^2.29.8",
    "typescript": "^5.7.2"
  }
}
