	"publishConfig": {
		"access": "public"
	},
	"keywords": [
		"cloudflare",
		"workers",
		"rag",
		"ai",
		"agents",
		"autorag"
	],
	"author": "Gabriel Massadas <https://github.com/g4brym>",
	"license": "MIT",
	"homepage": "https://github.com/cloudflare/ai",
	"repository": {
		"type": "git",
